{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "extends": ["react-app", "airbnb"],
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "semi": [1, "never"],
    "no-param-reassign": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": [
      "error",
      { "extensions": [".js", ".jsx"] }
    ],
    "import/prefer-default-export": "off",
    "react/jsx-one-expression-per-line": "off"
  }
}
